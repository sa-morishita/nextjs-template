{
  "name": "nextjs-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint --write --unsafe",
    "lint:fix": "biome lint --write --unsafe",
    "format": "biome format --write",
    "check": "biome check --write",
    "typecheck": "tsc --noEmit",
    "check:all": "pnpm run check && pnpm run typecheck",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.mts --reporter=verbose",
    "test:integration:watch": "vitest --config vitest.integration.config.mts",
    "test:storage": "vitest run --config vitest.storage.config.mts",
    "test:storage:watch": "vitest --config vitest.storage.config.mts",
    "test:all": "pnpm run test:unit && pnpm run test:integration && pnpm run test:storage",
    "test:coverage": "vitest run --coverage",
    "db:migrate:dev": "supabase db reset && drizzle-kit generate && drizzle-kit migrate && tsx src/db/setup-rls.ts && tsx src/db/seed.ts",
    "db:migrate:prod": "drizzle-kit generate && drizzle-kit migrate && tsx src/db/setup-rls.ts",
    "audit": "pnpm audit --audit-level=high"
  },
